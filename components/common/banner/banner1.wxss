.swipercontent {
    position: relative;
    padding: 20rpx 0rpx;
    background-color: #fff;
}
swiper{
    width: 100%;
    height:320rpx;
    border-radius: 14rpx;
    overflow: hidden;
}
swiper image {
    display: block;
    width: 100%;
    height: 100%;
    vertical-align: top;
    border-radius: 15rpx;
    overflow: hidden;
}
.swiper-img{box-sizing: border-box;padding: 0px 40rpx;height:320rpx;}
.dots{
    padding: 20rpx 40rpx;
    display: flex;
    justify-content: flex-end;
}
.dots-warp{
    display: -webkit-inline-flex;
    display: inline-flex;
    flex-direction: row;
    flex-wrap: nowrap;
    height: 12rpx;
    border-radius: 6rpx;
    background-color: #dedede;
    overflow: hidden;
}
.dot{
    width: 70rpx;
    height: 12rpx;
    background-color: transparent;
    border-radius: 6rpx;
    transition: all .6s;
}
.dot.active{
    background: #014282;
}
.dialog-mask{
  position: fixed;
    z-index: 1000;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.3);
}
.dialog-info{
    position: fixed;
    z-index: 5000;
    width: 80%;
    max-width: 600rpx;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    background-color: #FFFFFF;
    text-align: center;
    border-radius: 3px;
    overflow: hidden;
}
.dialog-title{
    font-size: 34rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    color: #111;
    background-color: #e6e6e6;
}
.dialog-content{
    padding: 0rpx 0rpx 50rpx;
    min-height: 80rpx;
    font-size: 32rpx;
    line-height: 1.3;
    word-wrap: break-word;
    word-break: break-all;
    color: #999999;
}
.dialog-footer{
    display: flex;
    align-items: center;
    position: relative;
    padding-top: 50rpx;
}
.dialog-btn{
    width: 400rpx;
    height: 90rpx;
    background-color: #ed6f2d;
    position: relative;
    color: #fff;
    font-size: 34rpx;
    text-align: center;
    line-height: 90rpx;
    margin: auto;
    border-radius: 45rpx;
}